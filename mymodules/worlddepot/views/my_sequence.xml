<odoo>
    <data noupdate="1">
        <record id="seq_container_package" model="ir.sequence">
            <field name="name">集装箱包裹序列</field>
            <field name="code">container.package.sequence</field>
            <field name="prefix">%(year)s</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
            <field name="implementation">standard</field>
        </record>
    </data>
    <data noupdate="1">
        <record id="seq_waybill" model="ir.sequence">
            <field name="name">Waboll No</field>
            <field name="code">seq.waybill</field>
            <field name="prefix">WB%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>
    </data>
    <data noupdate="1">
        <record id="seq_inbound_order" model="ir.sequence">
            <field name="name">Inbound Order No</field>
            <field name="code">seq.inbound.order</field>
            <field name="prefix">IO%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>
    </data>
    <data noupdate="1">
        <record id="seq_outbound_order" model="ir.sequence">
            <field name="name">Outbound Order No</field>
            <field name="code">seq.outbound.order</field>
            <field name="prefix">OO%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
        </record>
    </data>
</odoo>